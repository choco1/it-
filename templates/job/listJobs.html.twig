{% extends 'base.html.twig' %}

{% block body %}
    {% for job in jobs %}
        <h2>{{ job.title }}</h2>
        <p>{{ job.description }}</p>

        <a href="{{ path ('single_job', {'id': job.id}) }}">VOIR L'annonce </a>

        <a href="{{ path('delete_job', {'id': job.id}) }}">SUPPRIMER</a>
    {% endfor %}
{% endblock %}