{% extends 'base.html.twig' %}

{% block body %}
    {% for job in jobs %}
        <h2>{{ job.title }}</h2>
        <p>{{ job.discription }}</p>

        <a href="{{ path ('single_job', {'id': job.id}) }}"></a>

        <a href="{{ path('delete_job', {'id': job.id}) }}"></a>
    {% endfor %}
{% endblock %}